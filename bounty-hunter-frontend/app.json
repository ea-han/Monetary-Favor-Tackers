{
  "expo": {
    "name": "bounty-hunter-frontend",
    "slug": "bounty-hunter-frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.e7han1024.bountyhunterfrontend"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "assets/fonts/BaiJamjuree-Bold.ttf",
            "assets/fonts/BaiJamjuree-SemiBold.ttf",
            "assets/fonts/BaiJamjuree-Regular.ttf",
            "assets/fonts/BaiJamjuree-Medium.ttf"
          ]
        }
      ],
      "./util/securitySettings.js"
    ],
    "extra": {
      "eas": {
        "projectId": "baccbba5-422a-48fe-a7a4-6e11b13b6d07"
      }
    }
  }
}
